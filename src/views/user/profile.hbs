<div class="container text-center my-5"> 
   <a href="/links" class="btn btn-success">GO to Links</a>
</div>
<div class="container">
      
  <form action="profile" method="POST">
    <div class="row mt-4">
      {{#each data}}
        <div class="col-lg-4 col-md-6 col-8 m-auto">
          <div class="form-group">
            <label class="text-right" for="{{@key}}"><b><i>{{@key}} : </i></b></label>
            <input style="margin-top:-10px"  class="form-control form-control-sm" type="text" name="{{@key}}" value="{{this}}" />
          </div>
        </div>
      {{/each}}

    </div>
    <button type="submit" class="btn btn-success btn-block  col-md-6 col-8 m-auto">enviar</button>
    </form>
</div>